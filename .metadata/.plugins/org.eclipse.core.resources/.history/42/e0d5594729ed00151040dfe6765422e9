		System.out.println(tablo.length);
		tablo = new int[tablo.length + 1];
		System.out.println(tablo.length);
	}
	
	private Node[] nodes; 
	
	public static void add(int n) {
		this.temp = this.nodes;
		this.nodes = new Node[this.nodes.length + 1];
	}
	
